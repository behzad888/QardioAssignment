{
  "name": "qardio-assignment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next",
    "start:production": "next start",
    "start:server": "node server/index.js",
    "start:server:production": "NODE_ENV=production node server/index.js",
    "build": "next build",
    "test": "node scripts/tasks/test.js",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand --no-cache",
    "cypress": "cypress open",
    "prettier": "node ./scripts/prettier/index.js write-changed",
    "flow": "flow"
  },
  "dependencies": {
    "d3": "^5.9.7",
    "imagesloaded": "^4.1.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@zeit/next-sass": "^1.0.1",
    "add": "^2.0.6",
    "babel-cli": "^6.26.0",
    "babel-jest": "^24.8.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-app": "^9.0.0",
    "camelcase": "^5.2.0",
    "chalk": "^2.4.2",
    "cypress": "^3.3.1",
    "dotenv-load": "^2.0.0",
    "express": "^4.17.1",
    "flow-bin": "^0.102.0",
    "glob": "^7.1.4",
    "imagemin-mozjpeg": "^8.0.0",
    "jest": "24.7.1",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.7.1",
    "jest-watch-typeahead": "0.3.0",
    "next": "^8.1.0",
    "next-env": "^1.1.0",
    "next-fonts": "^0.17.0",
    "next-optimized-images": "^2.5.3",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react-app-polyfill": "^1.0.1",
    "react-test-renderer": "^16.8.6",
    "yarn": "^1.16.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/scripts/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/scripts/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
